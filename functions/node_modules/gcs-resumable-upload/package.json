{
  "name": "gcs-resumable-upload",
  "version": "0.10.2",
  "description": "Upload a file to Google Cloud Storage with built-in resumable behavior",
  "repository": "stephenplusplus/gcs-resumable-upload",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "bin": {
    "gcs-upload": "build/src/cli.js"
  },
  "scripts": {
    "test": "mocha build/test -r source-map-support/register",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "posttest": "npm run check",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "system-tests": "mocha build/test/system-tests -r source-map-support/register --timeout 20000"
  },
  "keywords": [
    "google",
    "gcloud",
    "storage",
    "gcs",
    "upload",
    "resumable"
  ],
  "files": [
    "build/src"
  ],
  "author": "Stephen Sawchuk <sawchuk@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "configstore": "^3.1.2",
    "google-auto-auth": "^0.10.0",
    "pumpify": "^1.4.0",
    "request": "^2.85.0",
    "stream-events": "^1.0.3"
  },
  "devDependencies": {
    "@types/configstore": "^2.1.1",
    "@types/is-stream": "^1.1.0",
    "@types/mocha": "^5.0.0",
    "@types/mockery": "^1.4.29",
    "@types/nock": "^9.1.3",
    "@types/node": "^9.6.1",
    "@types/request": "^2.47.0",
    "@types/through2": "^2.0.33",
    "through2": "^2.0.3",
    "gts": "^0.5.4",
    "is-stream": "^1.1.0",
    "mocha": "^5.0.5",
    "mockery": "^2.1.0",
    "nock": "^9.2.5",
    "source-map-support": "^0.5.4",
    "typescript": "~2.8.0"
  }
}
